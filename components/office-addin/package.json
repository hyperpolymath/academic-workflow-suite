{
  "name": "awap-office-addin",
  "version": "1.0.0",
  "description": "Academic Workflow Suite Office Add-in built with ReScript",
  "main": "dist/addin.js",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript build && webpack --mode production",
    "dev": "rescript build -w & webpack --mode development --watch",
    "test": "jest",
    "start": "office-addin-dev-server start manifest.xml",
    "stop": "office-addin-dev-server stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml"
  },
  "keywords": [
    "office-addin",
    "word",
    "rescript",
    "academic"
  ],
  "author": "Academic Workflow Suite",
  "license": "MIT",
  "dependencies": {
    "@rescript/core": "^1.6.1",
    "rescript": "^12.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@types/office-js": "^1.0.565",
    "@types/office-runtime": "^1.0.35",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.5",
    "office-addin-dev-certs": "^2.0.6",
    "office-addin-dev-server": "^1.13.2",
    "office-addin-manifest": "^2.1.2",
    "rescript-jest": "^0.10.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*_test.bs.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
